# ShadowCheck Environment Configuration
# Copy this file to .env and update with your secure values
# WARNING: Never commit .env file to version control

# Database Configuration
POSTGRES_PASSWORD=your_secure_postgres_password_here

# Grafana Monitoring
GRAFANA_PASSWORD=your_secure_grafana_password_here

# pgAdmin (Optional - only needed if using pgAdmin profile)
PGADMIN_PASSWORD=your_secure_pgadmin_password_here

# Optional: Database Connection Pool Settings
# DB_POOL_MIN=5
# DB_POOL_MAX=20
# DB_RETRY_ATTEMPTS=5
# DB_RETRY_DELAY=2000
